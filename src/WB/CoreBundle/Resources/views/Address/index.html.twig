<h1>Address list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Number</th>
            <th>Contact</th>
            <th>Info</th>
            <th>Street</th>
            <th>City</th>
            <th>Country</th>
            <th>Destrict</th>
            <th>Fon</th>
            <th>Fax</th>
            <th>Postcode</th>
            <th>Createdat</th>
            <th>Updatedat</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('address_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.number }}</td>
            <td>{{ entity.contact }}</td>
            <td>{{ entity.info }}</td>
            <td>{{ entity.street }}</td>
            <td>{{ entity.city }}</td>
            <td>{{ entity.country }}</td>
            <td>{{ entity.destrict }}</td>
            <td>{{ entity.fon }}</td>
            <td>{{ entity.fax }}</td>
            <td>{{ entity.postcode }}</td>
            <td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.updatedAt %}{{ entity.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('address_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('address_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('address_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
